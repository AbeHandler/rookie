CREATE OR REPLACE FUNCTION get_links(input character varying) RETURNS TABLE (S2 character varying) AS $$                                                                       BEGIN                                                                           RETURN QUERY select gramner.string from (select * from gramner join links on (links.gramner1=gramner.id) where gramner.string=input) as e inner join gramner on gramner2=gramner.id;                                                                             END;                                                                            $$ LANGUAGE plpgsql;